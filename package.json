{
    "name": "passport-shikimori",
    "version": "1.0.2",
    "description": "Passport strategy for authentication with Shikimori (shikimori.one)",
    "license": "MIT",
    "author": {
        "name": "Vladlen (Negezor)",
        "email": "negezor@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/negezor/passport-shikimori.git"
    },
    "homepage": "https://github.com/negezor/passport-shikimori#readme",
    "bugs": "https://github.com/negezor/passport-shikimori/issues",
    "keywords": [
        "passport",
        "shikimori",
        "auth",
        "authentication",
        "oauth2",
        "identity"
    ],
    "files": [
        "lib"
    ],
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "exports": {
        ".": {
            "import": "./lib/index.mjs",
            "require": "./lib/index.js"
        }
    },
    "sideEffects": false,
    "engines": {
        "node": ">=12.0.0"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.6.4",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.0.3",
        "@types/passport": "^1.0.7",
        "@types/passport-oauth2": "^1.4.11",
        "express": "^4.17.1",
        "jest": "^27.4.5",
        "passport": "^0.5.0",
        "rollup": "^2.61.1",
        "rollup-plugin-typescript2": "^0.31.1",
        "ts-jest": "^27.1.1",
        "typedoc": "^0.22.10",
        "typedoc-plugin-markdown": "^3.11.7",
        "typescript": "^4.5.4"
    },
    "scripts": {
        "prepare": "npm run rollup:build && npm run test",
        "build": "npm run rollup:build",
        "watch": "npm run rollup:watch",
        "clean": "rm -rf lib",
        "rollup:build": "rollup -c",
        "rollup:watch": "npm run rollup:build -- --watch",
        "test": "npm run test:jest && npm run lint",
        "test:jest": "jest --config jest.config.json --no-cache",
        "lint": "npm run lint:biome",
        "lint:biome": "biome lint --apply ./src",
        "docs:generate": "typedoc --theme markdown --out docs/api-reference --excludeExternals --readme none src"
    },
    "dependencies": {
        "passport-oauth2": "^1.6.1"
    }
}
